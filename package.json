    "prod": "yarn run production",
    "production": "cross-env NODE_ENV=production webpack-cli --mode=production",
    "analyse-development": "cross-env NODE_ENV=development webpack-cli --mode=development --profile --json",
    "analyse-production": "cross-env NODE_ENV=production webpack-cli --mode=production --profile --json",
    "analyse": "webpack-bundle-analyzer",
    "eslint": "eslint \"./src/assets/**/*.{js,jsx}\"",
    "stylelint": "stylelint \"./src/assets/**/*.css\"",
    "prettier": "prettier \"./src/assets/**/*.{js,jsx,json,css,yml,yaml}\" --list-different",
    "prettier-all": "prettier \"./**/*.{js,jsx,json,css,scss,less,yml,yaml}\" --list-different"
  },
